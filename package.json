{
  "name": "confz",
  "version": "0.3.0",
  "description": "A simple command line utility that creates files from templates and data.",
  "main": "index.js",
  "bin": {
    "confz": "./src/index.js"
  },
  "scripts": {
    "build": "docker build -t confz:latest -t bedrocksolutions/confz:latest .",
    "exec": "docker run bedrocksolutions/confz:latest",
    "pkg": "pkg . --options no-warnings --targets alpine,linux",
    "push": "docker push bedrocksolutions/confz:latest",
    "test": "node src/index.js --config ./confz.d/confz.yaml --printstack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BedrockSolutions/confz.git"
  },
  "author": "Bedrock Solutions",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/BedrockSolutions/confz/issues"
  },
  "homepage": "https://github.com/BedrockSolutions/confz#readme",
  "dependencies": {
    "ajv": "^6.10.0",
    "awaity": "^1.0.0",
    "bluebird": "^3.5.4",
    "chalk": "^2.4.2",
    "chokidar": "^2.1.5",
    "js-yaml": "^3.13.1",
    "json-schema-to-markdown": "^1.1.0",
    "lodash": "^4.17.11",
    "marked": "^0.6.2",
    "marked-terminal": "^3.2.0",
    "node-file-eval": "^1.0.0",
    "nunjucks": "^3.2.0",
    "verror": "^1.10.0",
    "winston": "^3.2.1",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "pkg": "^4.3.8",
    "prettier": "^1.17.0"
  }
}
